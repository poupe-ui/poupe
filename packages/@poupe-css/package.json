{
  "name": "@poupe/css",
  "version": "0.3.1",
  "type": "module",
  "description": "A TypeScript utility library for CSS property manipulation, formatting, and CSS-in-JS operations",
  "author": "Alejandro Mery <amery@apptly.co>",
  "license": "MIT",
  "homepage": "https://github.com/poupe-ui/poupe/tree/main/packages/@poupe-css#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poupe-ui/poupe.git",
    "directory": "packages/@poupe-css"
  },
  "bugs": {
    "url": "https://github.com/poupe-ui/poupe/issues"
  },
  "keywords": [
    "camelCase",
    "case-conversion",
    "css",
    "css-in-js",
    "css-properties",
    "css-rules",
    "css-utilities",
    "formatter",
    "kebab-case",
    "nested-css",
    "poupe",
    "stringify",
    "styling",
    "typescript"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "run-s lint type-check test build publint",
    "precommit": "run-s lint type-check test build",
    "dev:prepare": "unbuild --stub",
    "type-check": "tsc --noEmit",
    "lint": "env DEBUG=eslint:eslint eslint --fix .",
    "publint": "pnpx publint",
    "build": "unbuild",
    "test": "vitest run",
    "dev": "vitest watch",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@poupe/eslint-config": "^0.7.5",
    "@types/node": "^20.19.0",
    "eslint": "^9.29.0",
    "npm-run-all2": "^8.0.4",
    "publint": "^0.3.12",
    "typescript": "~5.8.3",
    "unbuild": "3.5.0",
    "vitest": "^3.2.4",
    "vue-tsc": "~2.2.10"
  },
  "engines": {
    "node": ">= 20.19.2",
    "pnpm": ">= 10.10.0"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39",
  "publishConfig": {
    "access": "public"
  }
}
