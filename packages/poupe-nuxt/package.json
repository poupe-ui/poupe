{
  "name": "@poupe/nuxt",
  "version": "0.1.1",
  "type": "module",
  "description": "Poupe's Nuxt module",
  "author": "Alejandro Mery <amery@apptly.co>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poupe-ui/poupe.git",
    "directory": "packages/poupe-nuxt"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "run-s lint type-check test build publint",
    "type-check": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "lint": "env DEBUG=eslint:eslint eslint --fix .",
    "publint": "pnpx publint",
    "build": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "clean:root": "rm -rf dist node_modules .nuxt",
    "clean:playground": "rm -rf playground/dist playground/node_modules playground/.nuxt",
    "clean": "run-s clean:root clean:playground"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.7.0",
    "@nuxt/eslint-config": "^1.0.0",
    "@nuxt/kit": "^3.15.4",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.4",
    "@nuxt/test-utils": "^3.15.4",
    "@poupe/eslint-config": "^0.4.14",
    "@types/node": "^22.13.0",
    "changelogen": "^0.5.7",
    "eslint": "^9.19.0",
    "nuxt": "^3.15.4",
    "typescript": "^5.6.3",
    "vite": "^6.0.11",
    "vitest": "^3.0.4",
    "vue-tsc": "^2.1.10"
  },
  "dependencies": {
    "@nuxt/icon": "^1.10.3",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/tailwindcss": "^6.13.1",
    "@poupe/theme-builder": "^0.5.8",
    "@poupe/vue": "^0.1.2",
    "stringify-object": "^5.0.0"
  }
}
