  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/poupe-ui/poupe",
    "directory": "packages/tailwind"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "style": "./dist/index.css",
        "import": "./dist/index.mjs"
      }
