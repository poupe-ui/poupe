{
  "name": "@poupe/vue",
  "version": "0.3.4",
  "type": "module",
  "description": "",
  "author": "Alejandro Mery <amery@apptly.co>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poupe-ui/poupe.git",
    "directory": "packages/poupe-vue"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.mjs"
    },
    "./resolver": {
      "types": "./dist/resolver.d.ts",
      "import": "./dist/resolver.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepack": "run-s lint type-check test build publint",
    "type-check": "vue-tsc --build --force",
    "lint": "env DEBUG=eslint:eslint eslint --fix .",
    "publint": "pnpx publint",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "test": "vitest run",
    "dev": "vite",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@iconify-json/svg-spinners": "^1.2.2",
    "@iconify/vue": "^4.3.0",
    "@unhead/vue": "^2.0.8",
    "reka-ui": "2.2.0",
    "tailwind-merge": "^3.2.0",
    "tailwind-scrollbar": "^4.0.2",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^3.4.17",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@poupe/eslint-config": "^0.5.2",
    "@poupe/theme-builder": "workspace:^",
    "@tsconfig/node20": "^20.1.5",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.17",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.1",
    "eslint-config-unjs": "0.4.2",
    "eslint-plugin-vue": "^10.0.0",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^7.0.2",
    "pathe": "^2.0.3",
    "postcss": "^8.5.3",
    "typescript": "~5.8.3",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.3.3",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-vue-devtools": "^7.7.5",
    "vitest": "^3.1.2",
    "vue-tsc": "^2.2.10"
  },
  "peerDependencies": {
    "@poupe/theme-builder": "^0.6.1"
  }
}
