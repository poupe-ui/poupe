{
  "name": "@poupe/vue",
  "version": "0.4.2",
  "type": "module",
  "description": "Vue component library for Poupe UI framework with theme customization and accessibility support",
  "author": "Alejandro Mery <amery@apptly.co>",
  "homepage": "https://poupe.dev",
  "license": "MIT",
  "keywords": [
    "components",
    "design-system",
    "material-design",
    "material-design-3",
    "poupe",
    "tailwindcss",
    "theming",
    "typescript",
    "ui-library",
    "vue",
    "vue3"
    ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poupe-ui/poupe.git",
    "directory": "packages/@poupe-vue"
  },
  "bugs": {
    "url": "https://github.com/poupe-ui/poupe/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.mjs"
    },
    "./resolver": {
      "types": "./dist/resolver.d.ts",
      "import": "./dist/resolver.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepack": "run-s lint type-check test build publint",
    "type-check": "vue-tsc --build --force",
    "lint": "env DEBUG=eslint:eslint eslint --fix .",
    "publint": "pnpx publint",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "test": "vitest run",
    "dev": "vite",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@iconify-json/svg-spinners": "^1.2.2",
    "@iconify/vue": "^5.0.0",
    "@poupe/tailwindcss": "workspace:^",
    "@poupe/theme-builder": "workspace:^",
    "@unhead/vue": "^2.0.8",
    "reka-ui": "2.2.1",
    "tailwind-merge": "^3.3.0",
    "tailwind-scrollbar": "^4.0.2",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.7",
    "vue": "^3.5.14"
  },
  "devDependencies": {
    "@poupe/eslint-config": "^0.6.3",
    "@tailwindcss/postcss": "^4.1.7",
    "@tailwindcss/vite": "^4.1.7",
    "@tsconfig/node20": "^20.1.5",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.47",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.26.0",
    "eslint-config-unjs": "0.4.2",
    "eslint-plugin-vue": "^10.1.0",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "pathe": "^2.0.3",
    "postcss": "^8.5.3",
    "publint": "^0.3.12",
    "typescript": "~5.8.3",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vitest": "^3.1.3",
    "vue-tsc": "^2.2.10"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.19.2",
    "pnpm": ">=10.10.0"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
