{
  "name": "@poupe/tailwindcss",
  "version": "0.4.0",
  "type": "module",
  "description": "TailwindCSS v4 plugin for Poupe UI framework with theme customization support",
  "author": "Alejandro Mery <amery@apptly.co>",
  "license": "MIT",
  "homepage": "https://poupe.dev",
  "keywords": [
    "css",
    "design-system",
    "design-tokens",
    "material-design",
    "material-design-3",
    "poupe",
    "tailwind",
    "tailwindcss",
    "theming",
    "typescript",
    "ui-framework"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poupe-ui/poupe.git",
    "directory": "packages/@poupe-tailwindcss"
  },
  "bugs": {
    "url": "https://github.com/poupe-ui/poupe/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "style": "./dist/style.css",
      "import": "./dist/index.mjs"
    },
    "./theme": {
      "types": "./dist/theme.d.ts",
      "import": "./dist/theme.mjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "style": "./dist/style.css",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepack": "run-s lint type-check test build publint",
    "dev:prepare": "unbuild --stub",
    "type-check": "tsc --noEmit",
    "lint": "env DEBUG=eslint:eslint eslint --fix .",
    "publint": "pnpx publint",
    "build": "unbuild",
    "test": "vitest run",
    "dev": "vitest watch",
    "clean": "rm -rf dist node_modules"
  },
  "devDependencies": {
    "@poupe/eslint-config": "workspace:^",
    "@tailwindcss/node": "^4.1.8",
    "@types/node": "^20.17.50",
    "eslint": "^9.27.0",
    "pathe": "^2.0.3",
    "publint": "^0.3.12",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "unbuild": "3.5.0",
    "vitest": "^3.2.0",
    "vue-tsc": "^2.2.10"
  },
  "dependencies": {
    "@poupe/css": "workspace:^",
    "@poupe/theme-builder": "workspace:^",
    "type-fest": "^4.41.0"
  },
  "peerDependencies": {
    "tailwindcss": "^4.1.8"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.19.2",
    "pnpm": ">=10.10.0"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
